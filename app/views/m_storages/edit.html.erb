<h1>Editing storage</h1>

<% form_for(@storage) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :warehouse %><br />
    <%= f.text_field :warehouse %>
  </p>
  <p>
    <%= f.label :material %><br />
    <%= f.text_field :material %>
  </p>
  <p>
    <%= f.label :avg_price %><br />
    <%= f.text_field :avg_price %>
  </p>
  <p>
    <%= f.label :qty %><br />
    <%= f.text_field :qty %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @storage %> |
<%= link_to 'Back', storages_path %>